{
  "name": "react-native-akar-icons",
  "version": "0.1.0",
  "engines": {
    "bun": "<1.3",
    "node": ">=20"
  },
  "description": "A React Native compatible package for Akar Icons",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/.*"
  ],
  "scripts": {
    "typecheck": "tsc",
    "lint:local": "eslint --config eslint.config.cjs --fix \"**/*.{js,ts,tsx, cjs}\"",
    "lint:ci": "eslint \"**/*.{js,ts,tsx, cjs}\"",
    "format:local": "prettier --config package.json --ignore-path .prettierignore --write .",
    "format:ci": "prettier --config package.json --ignore-path .prettierignore --check .",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "svg",
    "akar",
    "icons",
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tasta-app/react-native-akar-icons.git"
  },
  "author": "Guillaume <7831777+GuillaumeOj@users.noreply.github.com> (https://github.com/Tasta-app)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tasta-app/react-native-akar-icons/issues"
  },
  "homepage": "https://github.com/Tasta-app/react-native-akar-icons#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.1",
    "@eslint/compat": "^1.2.7",
    "@evilmartians/lefthook": "^1.11.3",
    "@react-native/eslint-config": "^0.73.2",
    "@release-it/conventional-changelog": "^9.0.4",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.18",
    "commitlint": "^17.8.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-builder-bob": "^0.37.0",
    "release-it": "^17.11.0",
    "typescript": "^5.8.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "organizeImportsSkipDestructiveCodeActions": true
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "languages": "js",
    "type": "library",
    "version": "0.48.3"
  }
}
